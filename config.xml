<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mymedifax.mobile" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Medifax</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="https://s3.amazonaws.com" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <access origin="https://s3.amazonaws.com" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="60" src="res/ios/Icon-App-60.png" width="60" />
        <icon height="120" src="res/ios/Icon-App-60@2x.png" width="120" />
        <icon height="76" src="res/ios/Icon-App-76.png" width="76" />
        <icon height="152" src="res/ios/Icon-App-76@2x.png" width="152" />
        <icon height="72" src="res/ios/Icon-App-72.png" width="72" />
        <icon height="144" src="res/ios/Icon-App-72@2x.png" width="144" />
        <icon height="167" src="res/ios/Icon-App-167.png" width="167" />
        <icon height="29" src="res/ios/Icon-App-small.png" width="29" />
        <icon height="58" src="res/ios/Icon-App-small@2x.png" width="58" />
        <icon height="50" src="res/ios/Icon-App-50.png" width="50" />
        <icon height="100" src="res/ios/Icon-App-50@2x.png" width="100" />
        <icon height="167" src="res/ios/Icon-App-83.5@2x.png" width="167" />
    </platform>
    <plugin name="cordova-plugin-hidden-statusbar-overlay" spec="https://github.com/katzer/cordova-plugin-hidden-statusbar-overlay" />
    <engine name="ios" spec="^4.5.4" />
    <engine name="android" spec="^7.0.0" />
</widget>
